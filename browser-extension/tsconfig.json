{
	"compilerOptions": {
		"strict": true,
		"target": "es6",
		"moduleResolution": "bundler",
		"module": "ES6",
		"esModuleInterop": true,
		"sourceMap": false,
		"rootDir": "src",
		"outDir": "dist/js",
		"noEmitOnError": true,
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"typeRoots": ["node_modules/@types"],
		"allowJs": true,
		"composite": true,
		"allowSyntheticDefaultImports": true
	}
}
